{
    "name": "anisaronno/laravel-settings",
    "description": "A Laravel package for managing application settings, similar to WordPress options.",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "laravel-settings",
        "application-settings",
        "WordPress",
        "Laravel",
        "wedevs"
    ],
    "authors": [
        {
            "name": "MD. Anichur Rahaman",
            "email": "contact@anichur.com",
            "homepage": "https://anichur.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/anisAronno/laravel-settings",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.13",
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "files": [
            "settings.php"
        ],
        "psr-4": {
            "AnisAronno\\LaravelSettings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnisAronno\\LaravelSettings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnisAronno\\LaravelSettings\\LaravelSettingsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
